function binary_serarch(arr, key) {
  var min = 0;
  var max = arr.length - 1

  while (min !== max) {
    var mid = Math.floor((max + min) / 2)
    if (arr[mid] === key) return mid

    if (arr[mid] < key) {
      max = mid - 1
    } 

    if (arr[mid] > key) {
      min = mid + 1
    }
  }

  return -1
}

console.log(binary_serarch([213,321,123,523,1234].sort(), 1234))